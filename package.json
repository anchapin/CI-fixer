{
  "name": "recursive-devops-agent",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "server": "tsx server.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run __tests__/unit",
    "test:integration": "vitest run __tests__/integration",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:ci": "npm run test:coverage && playwright test",
    "view-traces": "tsx scripts/view-traces.ts"
  },
  "dependencies": {
    "@e2b/code-interpreter": "latest",
    "@google/genai": "latest",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@playwright/test": "^1.57.0",
    "@prisma/client": "^5.22.0",
    "@rollup/rollup-win32-x64-msvc": "^4.53.3",
    "@tanstack/ai": "^0.0.3",
    "@tanstack/ai-react": "^0.0.3",
    "cors": "^2.8.5",
    "diff": "latest",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "handlebars": "^4.7.8",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "vitest": "^4.0.15",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.19.2",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "js-yaml": "^4.1.1",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
